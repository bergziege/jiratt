<UserControl x:Class="Jiratt.UI.Modules.NavBack.NavBackView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Jiratt.UI.Modules.NavBack"
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             xmlns:designViewModels="clr-namespace:Jiratt.UI.Modules.NavBack.DesignViewModels"
             xmlns:mvvm="http://prismlibrary.com/"
             mc:Ignorable="d" 
             mvvm:ViewModelLocator.AutoWireViewModel="True">
    <d:DesignProperties.DataContext>
        <designViewModels:NavBackDesignViewModel />
    </d:DesignProperties.DataContext>
    <Button Command="{Binding Path=NavigateBackCommand}">
        <materialDesign:PackIcon Kind="ArrowLeft" VerticalAlignment="Center"
                                 HorizontalAlignment="Center" />
    </Button>
</UserControl>
